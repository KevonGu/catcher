			</headermatchers>
			
			<valuematchers>
			
			</valuematchers>
			
		</tablecolumn>
		
		<!--是否通过预审查-->
		<tablecolumn name="IsCheck">
			<headermatchers>
				<matcher regular="是否通过"></matcher>
			</headermatchers>
			
			<valuematchers>
				<matcher regular="是">
					<replace from="是" to="1"></replace>
				</matcher>
				<matcher regular="否">
					<replace from="否" to="0"></replace>
				</matcher>
			</valuematchers>
		</tablecolumn>
		
		<!--技术得分-->
		<tablecolumn name="TechnologyScore">
			<headermatchers>
				<matcher regular="技术[\s\S]{0,4}得分"></matcher>
			</headermatchers>
			
			<valuematchers>
				<matcher regular="^[\s\S]{1,8}$" min="1">
					<replace from="[^\d\.]"></replace>
				</matcher>
			</valuematchers>
		</tablecolumn>
		
		<!--商务得分-->
		<tablecolumn name="BusinessScore">
			<headermatchers>
