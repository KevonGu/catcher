		<!--投标人-->
		<tablecolumn name="Bider">
		
			<headermatchers>
				<matcher regular="投标人名称"></matcher>
				<matcher regular="报价人名称"></matcher>
			</headermatchers>
			
			<valuematchers>
			
			</valuematchers>
			
		</tablecolumn>
		
		<!--是否通过预审查-->
		<tablecolumn name="IsCheck">
			<headermatchers>
				<matcher regular="是否通过"></matcher>
			</headermatchers>
			
			<valuematchers>
				<matcher regular="是">
					<replace from="是" to="1"></replace>
				</matcher>
				<matcher regular="否">
					<replace from="否" to="0"></replace>
				</matcher>
			</valuematchers>
		</tablecolumn>
		
		<!--技术得分-->
		<tablecolumn name="TechnologyScore">
			<headermatchers>
				<matcher regular="技术[\s\S]{0,4}得分"></matcher>
			</headermatchers>
			
			<valuematchers>
				<matcher regular="^[\s\S]{1,8}$" min="1">
					<replace from="[^\d\.]"></replace>
				</matcher>
			</valuematchers>
		</tablecolumn>
		
		<!--商务得分-->
		<tablecolumn name="BusinessScore">
			<headermatchers>
				<matcher regular="商务[\s\S]{0,4}得分"></matcher>
			
			</headermatchers>
			
			<valuematchers>
				<matcher regular="^[\s\S]{1,8}$" min="1">
					<replace from="[^\d\.]"></replace>
				</matcher>
			
			</valuematchers>
		</tablecolumn>
		
		<!--价格得分-->
		<tablecolumn name="PriceScore">
			<headermatchers>
				<matcher regular="价格[\s\S]{0,4}得分"></matcher>
			
			</headermatchers>
			
			<valuematchers>
				<matcher regular="^[\s\S]{1,8}$" min="1">
					<replace from="[^\d\.]"></replace>
				</matcher>
			
			</valuematchers>
		</tablecolumn>
		
		<!--综合得分-->
		<tablecolumn name="TotalScore">
			<headermatchers>
				<matcher regular="总得分"></matcher>
				<matcher regular="综合[\s\S]{0,4}得分"></matcher>
			
			</headermatchers>
				<matcher regular="^[\s\S]{1,8}$" min="1">
					<replace from="[^\d\.]"></replace>
				</matcher>
			
			<valuematchers>
			
			</valuematchers>
		</tablecolumn>
		
		<!--排名-->
		<tablecolumn name="Ranking">
			<headermatchers>
				<matcher regular="排名"></matcher>
				<matcher regular="名次"></matcher>
			</headermatchers>
			
			<valuematchers>
				<matcher regular="^[\s\S]{1,8}$" min="1">
					<replace from="[^\d]"></replace>
				</matcher>
			
			</valuematchers>
		</tablecolumn>
		
