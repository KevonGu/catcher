			</matcher>
			
			<matcher regular="(中标|成交).{0,6}(金额|总价|价格).{0,40}?[\d\.,]+" filter="tag-filter,trans-filter,sp-filter" min="3" max="20" check="\d{4,20}\.?\d{0,3}">
